<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Sample Action Set"
            visible="true"
            id="codeGen.actionSet">
         <menu
               label="Sample &amp;Menu"
               id="sampleMenu">
            <separator
                  name="sampleGroup">
            </separator>
         </menu>
         <action
               label="&amp;Code Generator"
               icon="icons/codeGen.jpg"
               class="codegen.actions.CodeGenAction"
               tooltip="代码生成工具"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="codegen.actions.CodeGenAction">
         </action>
      </actionSet>
   </extension>

</plugin>
